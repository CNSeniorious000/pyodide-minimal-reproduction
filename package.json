{
  "name": "pyodide-reproduction-template",
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "build": "vite build --sourcemap",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "postinstall": "echo export default {} > python/index.d.ts",
    "dev": "vite dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview"
  },
  "dependencies": {
    "@sveltejs/adapter-static": "^3.0.2",
    "@sveltejs/kit": "^2.5.18",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@unocss/extractor-svelte": "^0.61.3",
    "@unocss/reset": "^0.61.3",
    "pyodide": "0.26.1",
    "rollup-plugin-flatten-dir": "^1.0.1",
    "svelte": "^4.2.18",
    "svelte-sonner": "^0.3.25",
    "typescript": "^5.5.3",
    "unocss": "^0.61.3",
    "vite": "^5.3.3"
  },
  "stackblitz": {
    "installDependencies": false,
    "startCommand": "pnpm i --prod && pnpm svelte-kit sync && pnpm dev"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.22.0",
    "@types/node": "latest",
    "@unocss/eslint-plugin": "^0.61.3",
    "eslint": "^9.6.0",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-svelte": "^2.41.0",
    "svelte-check": "^3.8.4"
  }
}
