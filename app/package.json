{
  "name": "pyodide-reproduction-template",
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "build": "vite build --sourcemap",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "postinstall": "echo export default {} > ../python/index.d.ts",
    "dev": "vite dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview"
  },
  "dependencies": {
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.21.0",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@unocss/extractor-svelte": "^66.1.1",
    "@unocss/reset": "^66.1.1",
    "pyodide": "0.27.5",
    "rollup-plugin-flatten-dir": "^1.0.1",
    "svelte": "^4.2.19",
    "svelte-sonner": "^0.3.28",
    "typescript": "^5.8.3",
    "unocss": "^66.1.1",
    "vite": "^5.4.19"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.0",
    "@types/node": "latest",
    "@unocss/eslint-plugin": "^66.1.1",
    "eslint": "^9.26.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-svelte": "^3.5.1",
    "svelte-check": "^4.1.7"
  }
}
